{
  "name": "checkbox-tech-challenge",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "engines": {
    "node": ">=18.19.1"
  },
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build",
    "lint": "eslint",
    "format": "eslint --fix",
    "test": "npm run docker:test:up && NODE_OPTIONS=\"--import tsx\" vitest run && npm run docker:test:down",
    "test:watch": "npm run docker:test:up && NODE_OPTIONS=\"--import tsx\" vitest --bail 1",
    "migrate:up": "npx kysely migrate latest",
    "migrate:down": "npx kysely migrate down",
    "seed": "npx kysely seed run",
    "docker:test:up": "docker compose -f docker-compose.test.yaml up -d --build",
    "docker:test:down": "docker compose -f docker-compose.test.yaml down -v",
    "docker:dev:up": "docker compose -f docker-compose.dev.yaml up -d --build",
    "docker:dev:down": "docker compose -f docker-compose.dev.yaml down"
  },
  "dependencies": {
    "@mantine/core": "^7.17.2",
    "@mantine/dates": "^7.17.2",
    "@mantine/form": "^7.17.2",
    "@mantine/hooks": "^7.17.2",
    "@mantine/modals": "^7.17.2",
    "@tanstack/react-query": "^5.69.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "inversify": "^7.3.0",
    "kysely": "^0.27.6",
    "kysely-ctl": "^0.12.1",
    "mantine-datatable": "^7.17.1",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "vite-express": "*",
    "vitest": "^3.0.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.1.0",
    "@types/cors": "^2.8.17",
    "@types/date-fns": "^2.6.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@types/pg": "^8.11.11",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3",
    "rollup": "^4.14.0",
    "tslib": "^2.6.2",
    "tsx": "^4.3.0",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^5.0.2",
    "yarn": "^1"
  }
}